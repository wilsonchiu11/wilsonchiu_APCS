
public class DoMath{
	public static void main(String[] args){
		System.out.println (Calculate.square(5)); 
		System.out.println (Calculate.cube(-5));
		System.out.println (Calculate.average(5.0,2.0));
		System.out.println (Calculate.average(1.0,2.0,3.0));
		System.out.println (Calculate.toDegrees(2.0));
		System.out.println (Calculate.toRadians(180.0));
		System.out.println (Calculate.discriminant(2.0,1.0,3.0));
		System.out.println (Calculate.toImproperFrac(1,2,3));
		System.out.println (Calculate.toMixedNum(10,3));
		System.out.println (Calculate.foil(2,3,6,-7,"n"));
		System.out.println (Calculate.isDivisibleBy(10,5));
		System.out.println (Calculate.absValue(-15.0));
		System.out.println (Calculate.max(10,3));
		System.out.println (Calculate.max(10.0,11.0,12.0));
		System.out.println (Calculate.min(10,3));
		System.out.println (Calculate.round2(10.847628746287634287346287346285368));
		System.out.println (Calculate.exponent(10,0));
		
		
	}
}


