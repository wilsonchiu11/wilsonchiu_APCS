
public class DoMath{
	public static void main(String[] args){
		System.out.println (Calculate.square(5)); 
		System.out.println (Calculate.cube(-5));
		System.out.println (Calculate.average(5.0,2.0));
		System.out.println (Calculate.average(1.0,2.0,3.0));
		System.out.println (Calculate.toDegrees(2.0));
		System.out.println (Calculate.toRadians(180.0));
		System.out.println (Calculate.discriminant(2.0,1.0,3.0));
		System.out.println (Calculate.toImproperFrac(1,2,3));
	}
}


public static String toMixedNum (int number1,int number2){
	return((number1/number2)+"_"+(number1%number2)+"/"+number2));
}
public static String foil (int number1,int number2,int number3,int number4,"n")
	return(number1*number3)+"n^2+"+(number1*number4